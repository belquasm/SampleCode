@model Taclef.Authentication.Areas.Admin.Models.HomeIndexViewModel
<h1>Consumer Applications</h1>
<table class="table table-condensed table-hover table-striped">
	<thead>
		<tr>
			<th>Display Name</th>
			<th>Name</th>
			<th>ID</th>
			<th>Secret Key</th>
			<th>Website URL</th>
			<th>Success URL</th>
			<th>Fail URL</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var app in Model.Consumers) {
			<tr>
				<td>@app.DisplayName</td>
				<td>@app.Name</td>
				<td><input type="text" class="form-control" value="@app.Id" readonly="readonly"/></td>
				<td><input type="text" class="form-control" value="@app.SecretyKey" readonly="readonly"/></td>
				<td><a href="@app.WebsiteUrl" target="_blank">@app.WebsiteUrl</a></td>
				<td>@app.SuccessUrl</td>
				<td>@app.FailUrl</td>
			</tr>
		}
	</tbody>
</table>